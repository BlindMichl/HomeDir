<div class="panel panel-default">
	<div class="panel-heading">
		<h3 class="panel-title">Tags</h3>
	</div>
	<div class="panel-body">
	
		{{#each model}}
			<div class="panel panel-default">
				{{#link-to 'tag' this}}
					<div class="panel-body">

						<h3>{{this.name}}</h3>
						<p style="overflow: hidden; width: 200px; height:50px; background: linear-gradient(to bottom right, yellow , blue);">{{this.description}}</p>

				</div>
					{{/link-to}}
			</div>
		{{/each}}

	</div>
</div>