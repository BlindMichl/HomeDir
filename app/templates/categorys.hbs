<div class="panel panel-default">
	<div class="panel-heading">
		<h3 class="panel-title">Categorys</h3>
	</div>
	<div class="panel-body">
	
		{{#each model}}
			<div class="panel panel-default">
				{{#link-to 'category' this}}
					<div class="panel-body">

						<img class="pull-left" src="images/categorys/{{unbound this.picture}}" width="100" style="max-height:100px">
						<h3>{{this.name}}</h3>
						<p style="overflow: hidden; width: 200px; height:50px; background: linear-gradient(to bottom right, yellow , blue);">{{this.description}}</p>
						<ul class="list-inline">
							<li>
								<span class="label label-default">test Tag</span>
							</li>
							<li>
								<span class="label label-default">another tag</span>
							</li>
						</ul>

				</div>
					{{/link-to}}
			</div>
		{{/each}}

	</div>
</div>